{
  "name": "@orixen/runtimes",
  "version": "1.0.0",
  "description": "Code generation library for Orixen projects",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:typescript:base": "vitest run test/tests/shared test/tests/typescript/base",
    "test:typescript:aws": "vitest run test/tests/shared test/tests/typescript/aws --exclude '**/e2e/**'",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "test:e2e:typescript:aws": "vitest run --config vitest.e2e.config.ts test/tests/typescript/aws/e2e",
    "generate": "ts-node main.ts",
    "generate:local": "ts-node main.ts test-project local generate-lib",
    "generate:aws": "ts-node main.ts test-project aws deploy"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "handlebars": "^4.7.8",
    "yaml": "^2.3.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/orixen/orixen-open"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "Pedro Israel",
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@pinggy/pinggy": "^0.2.13",
    "@types/node": "^20.10.0",
    "@types/ws": "^8.18.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0",
    "vitest": "^3.0.0",
    "ws": "^8.19.0"
  }
}
